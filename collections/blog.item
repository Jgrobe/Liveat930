<section class="post-landing">
    <div class="fill-parent mask">
        <div class="landing-bg" style="background-image:url('{item.assetUrl}');"></div>
    </div>
    <h1 class="l-headline">{item.title}</h1>
    <a href="javascript:history.back();" class="post-landing-cta icon-arrow-reverse">{item.customContent.ctaLabel}</a>
</section>

<section class="post-content">
    {item.body}
</section>

{.section item}
    {@|apply module-share-this.block}
{.end}